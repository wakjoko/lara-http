{
    "name": "wakjoko/lara-http",
    "description": "Provides v7's Http Client for older Laravel versions up to v6",
    "license": "MIT",
    "authors": [{
        "name": "wakjoko",
        "email": "wakjoko@gmail.com"
    }],
    "require": {
        "guzzlehttp/guzzle": "^6|^7",
        "illuminate/support": ">=5.5"
    },
    "autoload": {
        "psr-4": {
            "Wakjoko\\LaraHttp\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wakjoko\\LaraHttp\\ServiceProvider"
            ],
            "aliases": {
                "LaraHttp": "Wakjoko\\LaraHttp\\Support\\Facades\\Http"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}